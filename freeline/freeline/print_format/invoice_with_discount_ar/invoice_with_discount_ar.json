 "html": "<style>\r\n\t.print-format table, .print-format tr, \r\n\t.print-format td, .print-format div, .print-format p, .print-format b {\r\n\t\t/*font-family: Tahoma;*/\r\n\t\tline-height: 100%;\r\n\t\tvertical-align: middle;\r\n\t}\r\n\t@media screen {\r\n\t\t.print-format {\r\n\t\t\twidth: 8.3in;\r\n\t\t\tpadding: 0.20in;\r\n\t\t\tmin-height: 11.7in;\r\n\t\t\t\r\n\t\t}\r\n\t}\r\n\t\r\n\r\n</style>\r\n<div style = \"padding-left:15px; padding-right:15px;\">\r\n    \r\n    <div class=\"row\" style=\"border-bottom: 1px solid black;\">\r\n        \r\n        <div class=\"col-xs-4\">\r\n            <div style = \"float:right; font-size:20px; padding-top: 65px;\">\r\n                <strong>{{ _(\"\u0641\u0627\u062a\u0648\u0631\u0629 \u0627\u0644\u0645\u0628\u064a\u0639\u0627\u062a\") }}</strong>\r\n                <br>\r\n                <strong class=\"mt-1\">{{ doc.name }}</strong>\r\n            </div>\r\n        </div>\r\n        <div class=\"col-xs-5\">\r\n            <div style = \"font-size:20px; padding-top: 65px; text-align: center;\">\r\n                \r\n                {% if doc.company == 'Tiejan Al Khaleej' %}\r\n                    <strong>{{ _(\"\u0634\u0631\u0643\u0629 \u062a\u064a\u062c\u0627\u0646 \u0627\u0644\u062e\u0644\u064a\u062c\") }}</strong>\r\n                {% else %}\r\n                   <strong class=\"mt-1\"> \u0634\u0631\u0643\u0629 \u062a\u064a\u062c\u0627\u0646 \u0627\u0644\u0631\u0627\u0641\u062f\u064a\u0646</strong>\r\n                {% endif %}\r\n            </div>\r\n        </div>\r\n        <div class=\"col-xs-3\">\r\n            <div>\r\n                <img class=\"rf-margin\" src=\"/private/files/Tiejan%20Logo_%20Black%202.jpg\" style=\"width: 100%; float: left;\">\r\n            </div>\r\n        </div>\r\n    </div>\r\n   \r\n    <div class=\"row mt-3\">\r\n        <div class=\"col-xs-6\">\r\n            <table>\r\n                <tr>\r\n                    <td style = \"white-space: pre;\">{{ _(\"\u0643\u0648\u062f \u0627\u0644\u0639\u0645\u064a\u0644\") }} </td>\r\n                    <td> : {{ _(doc.customer)}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td>{{ _(\"\u0627\u0633\u0645 \u0627\u0644\u0639\u0645\u064a\u0644\") }} </td>\r\n                    <td> : {{ _(doc.customer_name)}}</td>\r\n                </tr>\r\n                {% set district = frappe.db.get_value('Customer', doc.customer, 'district') %}\r\n                {% set city_ar = frappe.db.get_value('Customer', doc.customer, 'city_ar') %}\r\n                {% set employee_ar_name = frappe.db.get_value('Employee', doc.employee, 'altdesc') %}\r\n                <tr>\r\n                    <td style = \"white-space: pre;\">{{ _(\"\u0627\u0633\u0645 \u0627\u0644\u0645\u0646\u062f\u0648\u0628\") }} </td>\r\n                    <td> : {{ employee_ar_name }}</td>\r\n                </tr>\r\n                \r\n                {% if district %}\r\n                    <tr>\r\n                        <td>{{ _(\"\u0645\u0646\u0637\u0642\u0629\") }} </td>\r\n                        <td> : {{ _( district )}}</td>\r\n                    </tr>\r\n                {% endif %}\r\n                \r\n                {% if city_ar %}\r\n                    <tr>\r\n                        <td>{{ _(\"\u0645\u062f\u064a\u0646\u0629\") }} </td>\r\n                        <td> : {{ _(city_ar)}}</td>\r\n                    </tr>\r\n                {% endif %}\r\n                \r\n                {% if doc.contact_mobile %}\r\n                    <tr>\r\n                        <td>{{ _(\"\u0631\u0642\u0645 \u0627\u0644\u0645\u0648\u0628\u0627\u064a\u0644\") }}</td>\r\n                        <td> : {{ _(doc.contact_mobile)}}</td>\r\n                    </tr>\r\n                {% endif %}\r\n                \r\n                <!--<tr>-->\r\n                <!--    <td>{{ _(\"\u0627\u0633\u0645 \u0645\u0646\u062f\u0648\u0628 \") }}</td>-->\r\n                <!--    <td> : {{ employee_ar }}</td>-->\r\n                <!--</tr>-->\r\n            </table>\r\n        </div>\r\n        \r\n        <div class=\"col-xs-6\">\r\n            <table>\r\n                <tr>\r\n                    <td>{{ _(\"\u0634\u0631\u0643\u0629\") }}</td>\r\n                    <td> : {{ _(doc.company)}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td> {{ _(\"\u062a\u0627\u0631\u064a\u062e\") }}</td>\r\n                    <td> : {{ doc.posting_date }}</td>\r\n                </tr>\r\n\r\n                <tr>\r\n                    <td> {{ _(\"\u0639\u0645\u0644\u0629\") }}</td>\r\n                    <td> : {{ doc.currency }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td>{{ _(\"\u0627\u0633\u0645 \u0627\u0644\u0645\u0646\u062f\u0648\u0628\") }} </td>\r\n                    <td> : {{ employee_ar_name }}</td>\r\n                </tr>\r\n                \r\n            </table>\r\n        </div>\r\n    </div>\r\n    \r\n    <div class=\"row\">\r\n        <table style=\"font-size:12px; font-family: Tahoma;\" class=\"table mx-auto table-bordered\" id=\"items\">\r\n            <tr>\r\n                <th>{{ _(\"\u0631\u0642\u0645 \") }}</th>\r\n                <th>{{ _(\"\u0631\u0642\u0645 \u0627\u0644\u0645\u0627\u062f\u0629\") }}</th>\r\n                <th>{{ _(\"\u0645\u0627\u0631\u0643\u0629\") }}</th>\r\n                <th>{{ _(\"\u0627\u0633\u0645 \u0627\u0644\u0645\u0627\u062f\u0629 \") }}</th>\r\n                <th>{{ _(\"\u0628\u0627\u0631\u0643\u0648\u062f \") }}</th>\r\n                <th>{{ _(\"\u0643\u0645\u064a\u0629\") }}</th>\r\n                <th>{{ _(\"\u0648\u062d\u062f\u0629 \u0627\u0644\u0642\u064a\u0627\u0633\") }}</th>\r\n                <th>{{ _(\"\u0633\u0639\u0631 \u0627\u0644\u0645\u0627\u062f\u0629\") }}</th>\r\n                <th>{{ _(\"\u0642\u064a\u0645\u0629 \u0627\u0644\u062e\u0635\u0645 \") }}</th>\r\n                <th>{{ _(\"\u0627\u0644\u0633\u0639\u0631 \u0627\u0644\u0635\u0627\u0641\u064a \") }}</th>\r\n                <th>{{ _(\"\u0627\u0644\u0642\u064a\u0645\u0629 \u0627\u0644\u0635\u0627\u0641\u064a\u0629 \") }}</th>\r\n                <th width:10px;>{{ _(\"\u0639\u0627\u0645\u0644 \u062a\u062d\u0648\u064a\u0644\") }}</th>\r\n            </tr>\r\n            {% for d in doc.items %}\r\n                <tr>\r\n                    <td>{{ loop.index }}</td>\r\n                    <td>{{ d.item_code | replace(\"SI-\", \"\") }}</td>\r\n                    <td>{{ d.brand_code}}</td>\r\n                    <td>{{ d.item_arabic_name}}</td>\r\n                    <td>{{ d.barcode}}</td>\r\n                    <td>{{ d.qty}}</td>\r\n                    <td>{{ d.uom}}</td>\r\n                    <td>{{ d.price_list_rate}}</td>\r\n                    <td>{{ d.discount_amount}}</td>\r\n                    <td>{{ d.net_rate}}</td>\r\n                    <td>{{ d.net_amount}}</td>\r\n                    <td>{{ d.conversion_factor}}</td>\r\n                </tr>\r\n            {% endfor%}\r\n        </table>\r\n        <hr>\r\n    </div>\r\n   \r\n    <!--<div class=\"mt-3\">-->\r\n    <!--    <table>-->\r\n    <!--        <tr>-->\r\n    <!--            <td></td>-->\r\n    <!--        </tr>-->\r\n    <!--    </table>-->\r\n    <!--</div>-->\r\n    \r\n    <div class=\"row\">\r\n        \r\n        {% set line_total = get_line_total_disc(doc.name) %}\r\n        {% set gross_total = get_gross_total_amt(doc.name) %}\r\n        <div class=\"col-xs-6\">\r\n            <table style = \"float:right\">\r\n                <tr><td><strong>{{ _(\"\u0627\u0644\u0645\u062c\u0645\u0648\u0639\") }} :</strong></td><td>{{ gross_total | int }}</td></tr>\r\n                <tr><td><strong>{{ _(\"\u0645\u0642\u062f\u0627\u0631 \u0627\u0644\u062e\u0635\u0645\") }}: </strong></td><td>{{ line_total | int }}</td></tr>\r\n                <tr><td><strong>{{ _(\"\u062e\u0635\u0645 \u0625\u0636\u0627\u0641\u064a\") }}: </strong></td><td>{{ doc.discount_amount }}</td></tr>\r\n                <tr><td><strong>{{ _(\"\u0627\u0644\u0636\u0631\u0627\u0626\u0628 \u0648\u0627\u0644\u0631\u0633\u0648\u0645\") }}: </strong></td><td>{{ doc.total_taxes_and_charges }}</td></tr>\r\n                <tr><td><strong>{{ _(\"\u0625\u062c\u0645\u0627\u0644\u064a \u0627\u0644\u0645\u0628\u0644\u063a \u0627\u0644\u0645\u0633\u062a\u062d\u0642\") }}: </strong></td><td><b>{{ doc.grand_total }}</b></td></tr>\r\n            </table>\r\n        </div>\r\n        <div class=\"col-xs-6\">\r\n            {% set uom_detl = get_uom_qty_sum_inv(doc.name) %}\r\n            <table>\r\n                <tr>\r\n                    <td>{{ _(\"\u0627\u0644\u0643\u0645\u064a\u0629 \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a\u0629\") }} : {{doc.total_qty}}</td>\r\n                </tr>\r\n                {% for row in uom_detl %}\r\n                    <tr>\r\n                        <td>{{ _(row.uom) }} : {{row.qty }}</td>\r\n                    </tr>\r\n                {% endfor%}\r\n                <tr>\r\n                    <td>\u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u062a\u062c\u0647\u064a\u0632 : </td>\r\n                    <td>{{ doc.shipping_address }}</td>\r\n                </tr>\r\n                \r\n            </table>\r\n        </div>\r\n    </div>\r\n    <!--<div class=\"mt-3\">-->\r\n    <!--    {% set uom_detl = get_uom_qty_sum_inv(doc.name) %}-->\r\n    <!--    <table>-->\r\n    <!--        <tr>-->\r\n    <!--            <td>{{ _(\"\u0627\u0644\u0643\u0645\u064a\u0629 \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a\u0629\") }} : {{doc.total_qty}}</td>-->\r\n    <!--        </tr>-->\r\n    <!--        {% for row in uom_detl %}-->\r\n    <!--            <tr>-->\r\n    <!--                <td>{{ _(row.uom) }} : {{row.qty }}</td>-->\r\n    <!--            </tr>-->\r\n    <!--        {% endfor%}-->\r\n    <!--    </table>-->\r\n    <!--</div>-->\r\n    <!--<div class=\"mt-3\">-->\r\n    <!--    <table>-->\r\n    <!--        <tr>-->\r\n    <!--            <td><strong>{{ _(\"\u0625\u062c\u0645\u0627\u0644\u064a \u0627\u0644\u0645\u0628\u0644\u063a \u0627\u0644\u0645\u0633\u062a\u062d\u0642\") }} : {{ doc.grand_total }}</strong></td>-->\r\n    <!--        </tr>-->\r\n    <!--    </table>-->\r\n    <!--</div>-->\r\n    <hr>\r\n    <div class=\"row mt-3\">\r\n        <div class=\"col-xs-12\" style=\"text-align: center;\">\r\n            {% if doc.company == 'Tiejan Al Khaleej' %}\r\n                  <h4> \u0634\u0631\u0643\u0629 \u062a\u064a\u062c\u0627\u0646 \u0627\u0644\u062e\u0644\u064a\u062c : \u0627\u0631\u0628\u064a\u0644 - \u0627\u0645\u0628\u0627\u064a\u0631 T3-\u0637\u0627\u0628\u0642 \u0661- \u0645\u0643\u062a\u0628 \u0667</h4>\r\n            {% else %}\r\n                <h4>\u062d\u064a \u062d\u0637\u064a\u0646 - \u0634\u0627\u0631\u0639 \u0627\u0644\u0633\u062c\u0627\u062f - \u0628\u063a\u062f\u0627\u062f - \u0627\u0644\u0639\u0631\u0627\u0642</h4>\r\n            {% endif %}\r\n        </div>\r\n    </div>\r\n</div>",
