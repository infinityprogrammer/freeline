 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header -%}\r\n<style>\r\n\t.print-format table, .print-format tr, \r\n\t.print-format td, .print-format div, .print-format p, .print-format b {\r\n\t\tfont-family: Tahoma;\r\n\t\tline-height: 100%;\r\n\t\tvertical-align: middle;\r\n\t}\r\n\t@media screen {\r\n\t\t.print-format {\r\n\t\t\twidth: 8.3in;\r\n\t\t\tpadding: 0.20in;\r\n\t\t\tmin-height: 11.7in;\r\n\t\t\t\r\n\t\t}\r\n\t}\r\n\t\r\n\r\n</style>\r\n<div>\r\n    \r\n    {%- if not doc.get(\"print_heading\") and not doc.get(\"select_print_heading\")\r\n        and doc.set(\"select_print_heading\", _(\"Delivery Note \")) -%}{%- endif -%}\r\n    {{ add_header(0, 1, doc, letter_head, no_letterhead, print_settings) }}\r\n    \r\n    <div class=\"row\">\r\n        <div class=\"col-xs-6\">\r\n            <table>\r\n                <tr>\r\n                    <td>{{ _(\"\u0631\u0642\u0645 \u0627\u0644\u0639\u0645\u064a\u0644:\") }}</td>\r\n                    <td>{{ _(doc.customer)}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td>{{ _(\"\u0627\u0633\u0645 \u0627\u0644\u0639\u0645\u064a\u0644:\") }}</td>\r\n                    <td>{{ _(doc.customer_name)}}</td>\r\n                </tr>\r\n                {% set district = frappe.db.get_value('Customer', doc.customer, 'district') %}\r\n                {% set city_ar = frappe.db.get_value('Customer', doc.customer, 'city_ar') %}\r\n                {% set mobile_no = frappe.db.get_value('Customer', doc.customer, 'mobile_no') %}\r\n                \r\n                {% if district %}\r\n                    <tr>\r\n                        <td>\u0645\u0646\u0637\u0642\u0629 </td>\r\n                        <td>{{ district }}</td>\r\n                    </tr>\r\n                {% endif %}\r\n                {% if city_ar %}\r\n                    <tr>\r\n                        <td>\u0645\u062f\u064a\u0646\u0629 </td>\r\n                        <td>{{ city_ar }}</td>\r\n                    </tr>\r\n                {% endif %}\r\n                {% if mobile_no %}\r\n                    <tr>\r\n                        <td>\u0631\u0642\u0645 \u0627\u0644\u0645\u062d\u0645\u0648\u0644 </td>\r\n                        <td>{{ mobile_no }}</td>\r\n                    </tr>\r\n                {% endif %}\r\n            </table>\r\n        </div>\r\n        \r\n        <div class=\"col-xs-6\">\r\n            <table>\r\n                <!--<tr>-->\r\n                <!--    <td>{{ _(\"Company : \") }}</td>-->\r\n                <!--    <td>{{ _(doc.company)}}</td>-->\r\n                <!--</tr>-->\r\n                <tr>\r\n                    <td>{{ _(\"\u062a\u0627\u0631\u064a\u062e:\") }} </td>\r\n                    <td>{{ doc.posting_date }}</td>\r\n                </tr>\r\n\r\n                <tr>\r\n                    <td>{{ _(\"\u0637\u0644\u0628 \u0627\u0644\u0645\u0628\u064a\u0639\u0627\u062a \") }}</td>\r\n                    <td>{{ doc.items[0].against_sales_invoice }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td>{{ _(\" \u0641\u0627\u062a\u0648\u0631\u0629 \u0627\u0644\u0645\u0628\u064a\u0639\u0627\u062a\") }}</td>\r\n                    <td>{{ doc.items[0].against_sales_order }}</td>\r\n                </tr>\r\n            </table>\r\n        </div>\r\n    </div>\r\n    <div class=\"mt-3\">\r\n        <table class=\"table table-bordered table-condensed\">\r\n            <tr>\r\n                <th>{{ _(\" \u0631.\u062a\") }}</th>\r\n                <th>{{ _(\" \u0631\u0645\u0632 \u0627\u0644\u0633\u0644\u0639\u0629\") }}</th>\r\n                <th>{{ _(\" \u0627\u0633\u0645 \u0627\u0644\u0639\u0646\u0635\u0631 \u0627\u0644\u0639\u0631\u0628\u064a\") }}</th>\r\n                <th>{{ _(\"\u0643\u0645\u064a\u0629 \") }}</th>\r\n                <th>{{ _(\"\u0627\u0644\u0645\u0633\u062a\u0648\u062f\u0639\u0627\u062a \") }}</th>\r\n                <th>{{ _(\"\u0631\u0642\u0645 \u062f\u0641\u0639\u0629 \") }}</th>\r\n                <th>{{ _(\"\u0628\u0627\u0631\u0643\u0648\u062f \") }}</th>\r\n                <th>{{ _(\"\u0637\u0644\u0628 \u0627\u0644\u0645\u0628\u064a\u0639\u0627\u062a \") }}</th>\r\n                <th>{{ _(\" \u0641\u0627\u062a\u0648\u0631\u0629 \u0627\u0644\u0645\u0628\u064a\u0639\u0627\u062a\") }}</th>\r\n            </tr>\r\n            {% for d in doc.items %}\r\n                {% set bar = get_item_barcode(d.item_code) %}\r\n                <tr>\r\n                    <td>{{ loop.index }}</td>\r\n                    <td>{{ d.item_code}}</td>\r\n                    <td>{{ d.item_code}}</td>\r\n                    <td>{{ d.uom}} {{ d.qty}}</td>\r\n                    <td>{{ d.warehouse}}</td>\r\n                    <td>{{ d.batch_no}}</td>\r\n                    {% if bar %}\r\n                        <td>{{ bar }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                    <td>{{ d.against_sales_invoice}}</td>\r\n                    <td>{{ d.against_sales_order}}</td>\r\n                </tr>\r\n            {% endfor%}\r\n        </table>\r\n    </div>\r\n   \r\n    <div class=\"mt-3\">\r\n        {% set uom_detl = get_uom_qty_sum(doc.name) %}\r\n        <table>\r\n            <tr>\r\n                <td>{{ _(\"\u0627\u0644\u0643\u0645\u064a\u0629 \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a\u0629\") }} : {{doc.total_qty}}</td>\r\n            </tr>\r\n            {% for row in uom_detl %}\r\n                <tr>\r\n                    <td>{{ _(row.uom) }} : {{row.qty }}</td>\r\n                </tr>\r\n            {% endfor%}\r\n        </table>\r\n    </div>\r\n    \r\n</div>",
