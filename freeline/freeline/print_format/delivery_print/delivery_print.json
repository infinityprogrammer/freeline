 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header -%}\n<div>\n    \n    {%- if not doc.get(\"print_heading\") and not doc.get(\"select_print_heading\")\n        and doc.set(\"select_print_heading\", _(\"Delivery Note \")) -%}{%- endif -%}\n    {{ add_header(0, 1, doc, letter_head, no_letterhead, print_settings) }}\n    \n    <div class=\"row\">\n        <div class=\"col-xs-6\">\n            <table>\n                <tr>\n                    <td>{{ _(\"Customer Name : \") }}</td>\n                    <td>{{ _(doc.customer)}}</td>\n                </tr>\n                {% if doc.address_display %}\n                    <tr>\n                        <td>Address </td>\n                        <td>{{ doc.address_display }}</td>\n                    </tr>\n                {% endif %}\n            </table>\n        </div>\n        \n        <div class=\"col-xs-6\">\n            <table>\n                <tr>\n                    <td>{{ _(\"Company : \") }}</td>\n                    <td>{{ _(doc.company)}}</td>\n                </tr>\n                <tr>\n                    <td>Date </td>\n                    <td>{{ doc.posting_date }}</td>\n                </tr>\n\n                <tr>\n                    <td>Currency </td>\n                    <td>{{ doc.currency }}</td>\n                </tr>\n            </table>\n        </div>\n    </div>\n    <div class=\"mt-3\">\n        <table class=\"table table-bordered table-condensed\">\n            <tr>\n                <th>SL No</th>\n                <th>Item code</th>\n                <th>Item Arabic name</th>\n                <th>Barcode</th>\n                <th>Qty</th>\n                <th>Rate</th>\n                <th>Discount Amount</th>\n                <th>Amount</th>\n                <th>Net Amount</th>\n            </tr>\n            {% for d in doc.items %}\n                {% set bar = get_item_barcode(d.item_code) %}\n                <tr>\n                    <th>{{ loop.index }}</th>\n                    <th>{{ d.item_code}}</th>\n                    <th>{{ d.item_arabic_name}}</th>\n                    {% if bar %}\n                        <th>{{ bar }}</th>\n                    {% else %}\n                        <th></th>\n                    {% endif %}\n                    <th>{{ d.qty}}</th>\n                    <th>{{ d.rate}}</th>\n                    <th>{{ d.discount_amount}}</th>\n                    <th>{{ d.amount}}</th>\n                    <th>{{ d.net_amount}}</th>\n                </tr>\n            {% endfor%}\n        </table>\n    </div>\n   \n    <div class=\"mt-3\">\n        <table>\n            <tr>\n                <td>Net Total  : {{doc.total}}</td>\n            </tr>\n        </table>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-6\"></div>\n        <div class=\"col-xs-6\">\n            <table>\n                <tr><td><strong> Taxes & Charges :</strong></td><td>{{ doc.total_taxes_and_charges | abs }}</td></tr>\n                <tr><td><strong> Grand Total: </strong></td><td>{{ doc.grand_total | abs }}</td></tr>\n                <tr><td><strong> Rounded Total: </strong></td><td>{{ doc.rounded_total | abs }}</td></tr>\n            </table>\n        </div>\n    </div>\n</div>",
